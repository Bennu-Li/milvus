[Unit]
Description=Etcd of Milvus Standalone Server
After=network.target syslog.target
PartOf=milvus.service

[Install]
WantedBy=multi-user.target
Alias=milvus-etcd.service

[Service]
StandardOutput=journal
StandardError=inherit
Restart=always

# Start main service
ExecStart=/usr/bin/milvus-etcd --data-dir /var/lib/milvus/etcd-data --auto-compaction-retention 1000 --auto-compaction-mode revision --quota-backend-bytes 8589934592 --snapshot-count=50000 --enable-pprof=true